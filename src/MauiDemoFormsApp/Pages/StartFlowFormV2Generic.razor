@page "/start-flow-form-v2-generic/{flowName}/{Pk}"
@using BlazorForms.Rendering.MaterialBlazor.Components

<PcAuthorizeView>
    <DynamicForm FlowName="@FlowName" Pk="Pk" AllowAnonymousAccess="true" AllowFlowStorage="false" NavigationSuccess="/"/>
</PcAuthorizeView>

@code {
    [Parameter]
    public string FlowName { get; set; }

    [Parameter]
    public string Pk { get; set; }

}


