<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>

		<AssemblyVersion>1.9.5</AssemblyVersion>
		<PackageVersion>1.9.5</PackageVersion>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Copyright>Pro Coders PTY LTD</Copyright>
		<Description>BlazorForms.Rendering.Flows is a flow rendering library for BlazorForms framework that helps to create flows, forms and rules</Description>
		<Company>Pro Coders PTY LTD</Company>
		<PackageTags>blazor;flow;dynamic;form;forms;rule;framework</PackageTags>
		<Authors>Pro Coders PTY LTD</Authors>
	</PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="7.0.0" />
    <PackageReference Include="AutomaticGraphLayout.Drawing" Version="1.1.12" />
    <!--<PackageReference Include="System.Drawing.Common" Version="7.0.0" />-->
	  <PackageReference Include="Microsoft.Maui.Graphics.Skia" Version="6.0.501" />
	  <PackageReference Include="SkiaSharp" Version="2.88.3" />
	  <PackageReference Include="SkiaSharp.NativeAssets.Linux.NoDependencies" Version="2.88.3" />

  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\admin\BlazorForms.Admin.BusinessObjects\BlazorForms.Admin.BusinessObjects.csproj" />
    <ProjectReference Include="..\..\platform\BlazorForms.Platform\BlazorForms.Platform.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Georgia.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Roboto-Regular.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Verdana.ttf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
