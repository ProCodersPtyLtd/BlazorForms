{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DataProtection__UseLocalStore": "true"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5070"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DataProtection__UseLocalStore": "true"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7004;http://localhost:5070"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}